{
  "name": "grafana",
  "trademark": "Grafana",
  "installpath": "/data/wwwroot/grafana",
  "compose_file": "docker-compose-production.yml",
  "env_file": ".env_all",
  "requirements":{
    "cpu_arch": "Linux x86-64, ARM 32/64, x86/i686",
    "cpu": "1",
    "memory": "1",
    "disk": "20",
    "swap": "2",
    "url": "https://grafana.com/docs/grafana/latest/installation/requirements/#grafana-requirements"
  },

  "user": {
    "username": "admin",
    "password": "admin"
  },

  "services":[
    {
      "name": "grafana",
      "port": "9001",
      "description_cn": "浏览器访问 Grafana",
      "description_en": "HTTP to access to Grafana",
      "required": "Y"
    }
  ],

  "faqs": [
    {
      "question_cn": "grafana.ini 中的参数是否可以通过环境变量设置？",
      "answer_cn": "可以，格式：GF_[]name_参数名",
      "question_en": "Can I use environment for item of grafana.ini?",
      "answer_en": "Yes, the format is GF_[]name_itemname"
    }
  ]

}
